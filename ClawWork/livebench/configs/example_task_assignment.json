{
  "livebench": {
    "date_range": {
      "init_date": "2025-01-20",
      "end_date": "2025-01-24"
    },
    "economic": {
      "initial_balance": 1000.0,
      "task_values_path": "./scripts/task_value_estimates/task_values.jsonl",
      "token_pricing": {
        "input_per_1m": 2.5,
        "output_per_1m": 10.0
      }
    },
    "task_source": {
      "type": "jsonl",
      "path": "./livebench/data/tasks/example_tasks.jsonl"
    },
    "agents": [
      {
        "signature": "sequential-agent",
        "basemodel": "gpt-4o",
        "enabled": true,
        "task_assignment": {
          "mode": "sequential",
          "task_ids": ["custom-001", "custom-002", "custom-003"]
        }
      },
      {
        "signature": "random-agent",
        "basemodel": "gpt-4o",
        "enabled": true,
        "task_assignment": {
          "mode": "random",
          "task_ids": ["custom-004", "custom-005"]
        }
      }
    ],
    "agent_params": {
      "max_steps": 15,
      "max_retries": 3,
      "base_delay": 0.5
    },
    "data_path": "./livebench/data/agent_data"
  }
}
